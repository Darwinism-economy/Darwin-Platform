{
  "name": "darwin-launchpad-demo",
  "version": "1.0.0",
  "type": "module",
  "description": "Darwin Launchpad - Non-custodial, trustless, globally scalable token launchpad on Solana",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "anchor:build": "anchor build",
    "anchor:test": "anchor test",
    "anchor:deploy:local": "anchor deploy --provider.cluster localnet",
    "anchor:deploy:testnet": "anchor deploy --provider.cluster testnet",
    "anchor:deploy:mainnet": "anchor deploy --provider.cluster mainnet-beta",
    "deploy:local": "ts-node scripts/deploy.ts --cluster localnet",
